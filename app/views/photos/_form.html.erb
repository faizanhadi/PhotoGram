
<%= render 'shared/errors', obj: @photo %>

<div class="row">
  <div class ="well col-md-8 col-md-offset-2">
    
    <%= form_for @photo do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name%>
      
      <%= f.label :caption %>
      <%= f.text_area :caption%>
      
      <%= f.submit(@photo.new_record? ? 'Upload Photo' : 'Edit Photo', class: "btn btn-success")%>
    <% end%>
    
  </div>
</div>